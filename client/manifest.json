{
  "manifest_version": 3,
  "name": "Bookmark Manager Extension",
  "version": "1.0",
  "description": "Storing User DEtails",
  "permissions": ["bookmarks", "storage"],
  "host_permissions": ["http://localhost:5173/*"],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
